				</xenotypeChances>
			</xenotypeSet>
		</value>
	</Operation>
	<Operation Class="XmlExtensions.HybridPatch">
		<xpath>Defs/FactionDef[defName="CannibalPirate"]</xpath>
		<value>
			<xenotypeSet Operation="Safe">
				<xenotypeChances Operation="Safe">
					<WH_Sukiradae Operation="Add">0.025</WH_Sukiradae> <!-- 0.5x Default Xenotype Spawn Rate (0.05) -->
				</xenotypeChances>
			</xenotypeSet>
		</value>
	</Operation>
