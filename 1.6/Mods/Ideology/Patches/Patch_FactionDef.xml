		<value>
			<WH_Sukiradae Operation="Add">0.0125</WH_Sukiradae> <!-- 0.25x Default Xenotype Spawn Rate (0.05) -->
		</value>
	</Operation>
	<Operation Class="XmlExtensions.HybridPatch">
		<xpath>Defs/FactionDef[defName="TribeCannibal"]</xpath>
