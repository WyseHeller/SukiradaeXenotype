<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>WH_SukiradaeBody</defName>
		<label>piñiatafur</label>
		<labelShortAdj>piñiatafurred</labelShortAdj>
		<description>Carriers of this gene have stubby little feathers covering their corrupted bodies. This provides some resistance to the heat, but also lets in the cold.</description>
		<iconPath>UI/Icons/Genes/WH_SukiradaeBody</iconPath>
		<displayCategory>Miscellaneous</displayCategory> <!-- Furskin (Miscellaneous) -->
		<displayOrderInCategory>401</displayOrderInCategory> <!-- Furskin (400) -->
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<exclusionTags>
			<li>AG_Bodies</li>
			<li>Bodies</li>
			<li>Body</li>
			<li>Fur</li>
			<li>EBSG_MeatReplacer</li>
			<li>Flesh</li>
			<li>Meat</li>
			<li>AG_Meat</li>
			<li>EBSG_LeatherReplacer</li>
			<li>Leather</li>
			<li>Leathers</li>
			<li>AG_Leather</li>
		</exclusionTags>
		<fur>WH_SukiradaeBody</fur>
		<skinIsHairColor>true</skinIsHairColor>
		<tattoosVisible>false</tattoosVisible>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_Fur</nodeClass>
				<workerClass>PawnRenderNodeWorker_Fur</workerClass>
				<baseLayer>5</baseLayer>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<colorType>Skin</colorType>
				<parentTagDef>Body</parentTagDef>
			</li>
		</renderNodeProperties>
		<statOffsets>
			<ComfyTemperatureMax>25</ComfyTemperatureMax>
		</statOffsets>
		<modExtensions>
			<li Class="EBSGFramework.ButcherProductExtension">
				<meatReplacement>Meat_Twisted</meatReplacement>
				<leatherReplacement>Leather_Bird</leatherReplacement>
			</li>
			<li Class="VEF.Genes.GeneExtension">
				<defsTreatedAsHumanMeat>
					<li>Meat_Twisted</li>
				</defsTreatedAsHumanMeat>
				<defsTreatedAsHumanLeather>
					<li>Leather_Bird</li>
				</defsTreatedAsHumanLeather>
			</li>
		</modExtensions>
	</GeneDef>
	<GeneDef ParentName="GeneHeadboneBase">
		<defName>WH_StraightHorns</defName>
		<label>straight-horns</label>
		<description>Carriers of this gene grow two straight horns protruding from the forehead.</description>
		<iconPath>UI/Icons/Genes/WH_StraightHorns</iconPath>
		<displayOrderInCategory>111</displayOrderInCategory> <!-- Mini-Horns (110) -->
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Things/Pawn/Humanlike/HeadAttachments/WH_StraightHorns/StraightHorns</texPath>
				<parentTagDef>Head</parentTagDef>
				<drawData>
				<defaultData>
					<layer>80</layer>
				</defaultData>
				<dataNorth>
					<layer>10</layer>
				</dataNorth>
				</drawData>
			</li>
		</renderNodeProperties>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>horn</symbol></li>
				<li><symbol>knob</symbol></li>
				<li><symbol>spike</symbol></li>
				<li><symbol>devil</symbol></li>
				<li><symbol>demon</symbol></li>
				<li><symbol>luci</symbol></li>
				<li><symbol>beelze</symbol></li>
				<li><symbol>hell</symbol></li>
		  </prefixSymbols>
		  <suffixSymbols>
				<li><symbol>horn</symbol></li>
				<li><symbol>spike</symbol></li>
				<li><symbol>vil</symbol></li>
		  </suffixSymbols>
		</symbolPack>
	</GeneDef>
	<GeneDef ParentName="GeneTailBase">
		<defName>WH_CurledTail</defName>
		<label>curled tail</label>
		<description>Carriers of this gene grow a slender, curled tail.</description>
		<iconPath>UI/Icons/Genes/WH_CurledTail</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<displayCategory>Miscellaneous</displayCategory> <!-- SmoothTail (Miscellaneous) -->
		<displayOrderInCategory>1001</displayOrderInCategory> <!-- SmoothTail (1000) -->
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<renderNodeProperties>
			<li>
				<workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
				<texPath>Things/Pawn/Humanlike/BodyAttachments/WH_CurledTail/CurledTail</texPath>
				<colorType>Skin</colorType>
				<overrideMeshSize>(1, 1)</overrideMeshSize>
				<parentTagDef>Body</parentTagDef>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<drawData>
					<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
					<defaultData>
						<layer>-2</layer>
					</defaultData>
					<dataNorth>
						<offset>(0, 0, -0.15)</offset>
						<layer>90</layer>
					</dataNorth>
					<dataSouth>
						<offset>(-0, 0, -0.15)</offset>
					</dataSouth>
					<dataEast>
						<offset>(-0.5, 0, -0.15)</offset>
					</dataEast>
					<dataWest>
						<offset>(0.5, 0, -0.15)</offset>
					</dataWest>
				</drawData>
			</li>
		</renderNodeProperties>
	</GeneDef>
</Defs>